<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/expandable_listview_child_linearlayout"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:paddingTop="12dp"
    android:paddingBottom="12dp"
    android:orientation="horizontal">
    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:paddingStart="24dp"
        android:paddingEnd="8dp"
        android:textSize="14sp"
        android:gravity="center_vertical"
        android:id="@+id/expandable_listview_child_textview_testId"
        android:text="@string/test_id"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:paddingStart="8dp"
        android:paddingEnd="8dp"
        android:textSize="14sp"
        android:gravity="center_vertical"
        android:id="@+id/expandable_listview_child_textview_patientId"
        android:text="@string/patient_id"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:paddingStart="8dp"
        android:paddingEnd="24dp"
        android:textSize="14sp"
        android:gravity="center_vertical"
        android:id="@+id/expandable_listview_child_textview_datetime"
        android:text="Date Time"/>
</LinearLayout>